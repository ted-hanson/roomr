<h1>Create Your Listing!!!</h1>

<%= form_for :listing, url: listings_path  do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  
  <p>
    <%= f.label :neighborhood %><br>
    <%= f.text_area :neighborhood %>
  </p>
  
  <p>
    <%= f.label :dateavailable_month %><br>
    <%= f.text_area :dateavailable_month %>
  </p>
  <p>
    <%= f.label :dateavailable_day %><br>
    <%= f.text_area :dateavailable_day %>
  </p>
  <p>
    <%= f.label :dateavailable_year %><br>
    <%= f.text_area :dateavailable_year %>
  </p>
  <p>
    <%= f.label :price %><br>
    <%= f.text_area :price %>
  </p>

  <p>
    <%= f.label :numbedrooms %><br>
    <%= f.text_area :numbedrooms %>
  </p>
  <p>
    <%= f.label :numbathrooms %><br>
    <%= f.text_area :numbathrooms %>
  </p>
  <p>
    <%= f.label :photos %><br>
    <%= f.text_area :photos %>
  </p>

 
  <p>
    <%= f.submit %>
  </p>
<% end %>